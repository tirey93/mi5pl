msgctxt "756_756_0_BUGEYE"
msgid "{stern}I vote yes, as much as it pains me."
msgstr "Z wielkim bólem głosuję na \"tak\"."

msgctxt "757_757_0_GUYBRUSH"
msgid "{happy}Good man!"
msgstr "Dobrze!"

msgctxt "758_758_0_BUGEYE"
msgid "{suspicious}Brother Moose, Brother Santino, still yes?"
msgstr "Bracia Muszalu i Santino, wciąż na \"tak\"?"

msgctxt "759_759_0_MURRAY"
