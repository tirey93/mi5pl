msgstr "> Usłysz pytanie ponownie"

msgctxt "2109_0_GUYBRUSH"
msgid "[pickup1] Don't worry, I'm on it."
msgstr "[pickup1] Już się za to zabieram."

msgctxt "2110_0_GUYBRUSH"
msgid "{happy}[pickup1] I'll add that to my pirate face repertoire!{default}"
msgstr "{happy}[pickup1]Dodam to do mojego repertuaru pirackich min!{default}"

msgctxt "2111_0_GUYBRUSH"
msgid "[pickup1] It's locked."
msgstr "[pickup1] Zamknięte."

msgctxt "2112_0_GUYBRUSH"
msgid "[pickup1] My hook is pretty handy...{worried}[sigh] but sometimes I miss the ol' opposable digits."
msgstr "[pickup1] Mój hak jest bardzo poręczny...{worried}[sigh] choć czasami tęsknię za przeciwstawnym palcem."

msgctxt "2113_0_GUYBRUSH"
msgid "[pickup1] I'm pretty sure this stuff only works on giant manatees.  And besides... {Disgust} ick."
msgstr "[pickup1] Jestem pewien, że ta rzecz działa tylko na gigantyczne manaty.  A poza tym... cuchnie."

msgctxt "2114_0_GUYBRUSH"
msgid "{confused}[pickup1] [underwater] Well, that was...{disgust} actually, I don't wanna talk about it.{default}"
msgstr "{confused}[pickup1] [underwater] Cóż, to było...{disgust} prawdę mówiąc, nie chcę o tym mówić.{default}"

msgctxt "2115_0_GUYBRUSH"
msgid "[pickup1] [to himself] [commenting on his bongo playing] I'm good, but I'm no substitute for Noogie."
msgstr "[pickup1] [to himself] [commenting on his bongo playing] Dobry jestem, ale nie zastąpię Noogiego."

msgctxt "2116_0_GUYBRUSH"
msgid "[pickup1] I've already got some."
msgstr "[pickup1] Już trochę mam."

msgctxt "2117_0_GUYBRUSH"
msgid "[pickup1] Nope, I can't reach it that way.  It's just too far down."
msgstr "Nie sięgnę tego w ten sposób.  Jest za daleko."

msgctxt "2118_0_GUYBRUSH"
msgid "{confused}[pickup1] Mug of bile?"
msgstr "{confused}[pickup1] Kubek z żółcią?"

msgctxt "2119_0_GUYBRUSH"
msgid "[pickup1] There's nothing interesting up there now.  Just a whole lotta tongue."
msgstr "[pickup1] Nie ma tam nic ciekawego.  Tylko cały bezmiar języka."

msgctxt "2120_0_"
msgid "Think I could count on your vote into the Brotherhood?"
msgstr "Myślisz, że mogę liczyć na twój głos do Bractwa?"

msgctxt "2121_0_"
msgid "Thanks for your vote, bra."
msgstr "Dzięki za twój głos, ziom."

msgctxt "2122_0_GUYBRUSH"
msgid "{happy}[pickup1] Hey there Mu{worried} -- er, Santino."
msgstr "{happy}[pickup1] Witaj, Mu... znaczy Santino."

msgctxt "2123_0_MURRAY"
msgid "{angry}[pickup1] Aloha."
msgstr "{angry}[pickup1] Aloha."

msgctxt "2124_0_"
msgid "Give me your vote, or else!"
msgstr "Daj mi swój głos albo po tobie!"

msgctxt "2125_0_GUYBRUSH"
msgid "[pickup1] Ahoy there."
msgstr "[pickup1] Cześć."

msgctxt "2126_0_BUGEYE"
msgid "[pickup1] Now what?"
msgstr "[pickup1] Czego chcesz?"

msgctxt "2128_0_GUYBRUSH"
msgid "{happy}[pickup1] Thanks for voting me in, Murray.  I got the cochlea!"
msgstr "{happy}[pickup1] Dzięki za twój głos, Murray.  Mam ślimaka!"

msgctxt "2129_0_MURRAY"
msgid "{confused}[pickup1] That's some kind of horrible disease, right?"
msgstr "{confused}[pickup1] To jakiś rodzaj strasznej choroby, tak?"

msgctxt "2130_0_GUYBRUSH"
msgid "{confused}[pickup1] Not exactly."
msgstr "{confused}[pickup1] Niezupełnie."

msgctxt "2131_0_"
msgid "Hooded..."
msgstr "Okapnięta..."

msgctxt "2143_0_"
msgid "?"
msgstr "?"

msgctxt "2144_0_"
msgid "Stinky..."
msgstr "Śmierdząca..."

msgctxt "2146_0_"
msgid "Bugged Out..."
msgstr "Zbzikowana..."

msgctxt "2147_0_"
msgid "Nasty..."
msgstr "Ochydna..."

msgctxt "2148_0_"
msgid "Fish-eyed..."
msgstr "Rybio-ocza..."

msgctxt "2149_0_"
msgid "...Cross-eyed..."
msgstr "... Zezowata..."

msgctxt "2150_0_"
msgid "...Lazy-eyed..."
msgstr "... Leniowo-ocza..."

msgctxt "2151_0_"
msgid "...Tweaking..."
msgstr "... Wykręcona..."

msgctxt "2152_0_"
