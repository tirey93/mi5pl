msgid "Wait, let me think..."
msgstr "Niech pomyślę..."

msgctxt "2104_0_GUYBRUSH"
msgid "[pickup1] {Confused} Could you repeat that?"
msgstr "[pickup1] {Confused} Mógłbyś powtórzyć?"

msgctxt "2105_0_"
msgid "> Remember Morgan's tattoo"
msgstr "> Przypomnij sobie tatuaż Morgan"

msgctxt "2106_0_"
msgid "> Remember Morgan's sword"
msgstr "> Przypomnij sobie miecz Morgan"

msgctxt "2107_0_"
