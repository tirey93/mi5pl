msgstr "To słodkie, ale nie zapomniałeś o czymś?"

msgctxt "835_835_0_GUYBRUSH"
msgid "{worried}[Running] Huh?{confused} What? {surprised}[Realizing] Oh!"
msgstr "O czym?"

msgctxt "836_836_0_GUYBRUSH"
msgid "{happy}[Running] Weapon privileges restored!"
msgstr "Zakaz broni zniesiony!"

msgctxt "837_837_0_MORGAN"
