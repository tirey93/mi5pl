msgctxt "649_649_0_GUYBRUSH"
msgid "{happy}Nice technique."
msgstr "Niezła metoda."

msgctxt "650_650_0_GUYBRUSH"
msgid "{happy}Thanks for your vote...{confused}um...{embarrassed}bra."
msgstr "Dzięki za twój głos... brachu."

msgctxt "651_651_0_MOOSE"
