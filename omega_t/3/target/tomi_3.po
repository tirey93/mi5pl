msgctxt "829_829_0_GUYBRUSH"
msgid "{worried}[Running] Look, I'm sorry about before."
msgstr "Słuchaj, przepraszam za tamto."

msgctxt "830_830_0_MORGAN"
msgid "{Sarcastic}Sorry about what?"
msgstr "Za co?"

msgctxt "831_831_0_GUYBRUSH"
