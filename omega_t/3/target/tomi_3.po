msgctxt "630_630_0_GUYBRUSH"
msgid "Ah. Right on.{suspicious} Dude."
msgstr "No lekko, ziom."

msgctxt "631_631_0_MOOSE"
msgid "{happy}Can I interest you in a beverage?"
msgstr "Może skusisz się na drina?"

msgctxt "632_632_0_GUYBRUSH"
msgid "{confused}Is there a substantial amount of lead in your drink?"
