msgid "Isle of Ewe"
msgstr "Wysepka Łu"

msgctxt "1808_0_"
msgid "Isle of Ewe"
msgstr "Wysepka Łu"

msgctxt "1798_0_"
msgid "Flotsam Island"
msgstr "Wyspa Rozbitków"

msgctxt "1799_0_"
msgid "Jerkbait Islands"
msgstr "Wyspy Sharpnęta"

