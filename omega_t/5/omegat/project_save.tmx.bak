      </tuv>
      <tuv lang="pl">
        <seg>Grób Szkrabróda</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en-US">
        <seg>No</seg>
      </tuv>
      <tuv lang="pl">
        <seg>Nie</seg>
      </tuv>
    </tu>
    <tu>
